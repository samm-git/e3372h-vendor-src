vpath %.c $(WEBSDK_ROOT)/qrcode/src



#==============================================================================
#*   在下面添加需要编译的代码文件名称
#==============================================================================
WEBSDK_LIBQRENCODE_SOURCES=\
	bitstream.c \
	mask.c \
	mmask.c \
	mqrspec.c \
	qrencode.c \
	qrinput.c \
	qrspec.c \
	rscode.c \
	split.c 

WEBSDK_QRCODE_OBJ :=  $(addprefix $(TARGETDIR)/, $(WEBSDK_LIBQRENCODE_SOURCES:%.c=%.o))
WEBSDK_OBJ_LIST += $(WEBSDK_QRCODE_OBJ)

